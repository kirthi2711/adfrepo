{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kirthidatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"Ls_development_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_development'"
		},
		"Ls_production_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_production'"
		},
		"SqlServer1linkedservices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1linkedservices'"
		},
		"kirthiserver1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'kirthiserver1'"
		},
		"selfhosted_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'selfhosted'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kirthidatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"Ls_development_connectionString": {
						"value": "[parameters('Ls_development_connectionString')]"
					},
					"Ls_production_connectionString": {
						"value": "[parameters('Ls_production_connectionString')]"
					},
					"kirthiserver1_connectionString": {
						"value": "[parameters('kirthiserver1_connectionString')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SqlServer1linkedservices_connectionString": {
						"value": "[parameters('SqlServer1linkedservices_connectionString')]"
					},
					"selfhosted_connectionString": {
						"value": "[parameters('selfhosted_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}